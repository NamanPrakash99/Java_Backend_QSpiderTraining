package com.prac;

public class Employee1 {
	int id;
	String name;
	double salary;
	int age;
	String dept;
	String Dob;
	public Employee1(int id, String name, double salary, int age, String dept, String dob) {
		this.id = id;
		this.name = name;
		this.salary = salary;
		this.age = age;
		this.dept = dept;
		Dob = dob;
	}
	
	public boolean isValidAge() {
	    return age > 17;
	}

	
	
}
