	@Test
	public void testValidName() {
		Employee r=new Employee(20, "1233", 34, "IT");
		assertTrue(r.isValidName());
	}
	
	@Test
	public void testValidDept() {
		Employee a=new Employee(20, "Naman", 34, "I");
		assertTrue(a.isValidDept());
