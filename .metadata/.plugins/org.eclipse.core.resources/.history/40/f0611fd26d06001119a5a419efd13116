package com.practice;

public class productMain {
	public static void main(String[] args) {

			 ProductDao dao = new ProductDao();

		        Product p = new Product();
		        p.setId(5);
		        p.setName("Red-Magic 10 X");
		        p.setQuantity(6);
		        p.setPrice(120000);

		        dao.insert(p);

		        p.setPrice(55000);
		        p.setQuantity(10);
		        dao.update(p);

		        System.out.println(dao.find(1));

		        dao.delete(1);
	}
}
